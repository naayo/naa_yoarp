    <validationRules>
        <fullName>Charge_et_date_test_a_renseigner</fullName>
        <active>false</active>
        <description>Veuillez renseigner la charge et la date en test</description>
        <errorConditionFormula>AND( $User.ByPassValidationRule__c =False,
TEXT( Responsable__c ) = &quot;Kerensen&quot; &amp;&amp; 
(TEXT( Date_en_test__c ) = &quot;&quot; ||  TEXT(Charge_J__c) = &quot;&quot;) &amp;&amp;
( $User.LastName = &quot;CONFIG&quot; || 
 $User.LastName = &quot;DEVELOPPEMENT&quot; || 
 $User.LastName = &quot;DEV (MAROC)&quot; || 
 $User.LastName = &quot;OMRAN&quot; || 
 $User.LastName = &quot;LE GRAND&quot;))</errorConditionFormula>
        <errorDisplayField>Date_en_test__c</errorDisplayField>
        <errorMessage>Veuillez renseigner la charge et la date en test</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_limite_a_renseigner</fullName>
        <active>false</active>
        <description>Veuillez renseigner la date limite</description>
        <errorConditionFormula>TEXT(Type__c) &lt;&gt; &quot;Version&quot; &amp;&amp; TEXT( Responsable__c ) = &quot;K par K&quot; &amp;&amp; TEXT(Date_limite__c) = &quot;&quot;</errorConditionFormula>
        <errorDisplayField>Date_limite__c</errorDisplayField>
        <errorMessage>Veuillez renseigner la date limite</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_test_non_modifiable_par_K_par_K</fullName>
        <active>false</active>
        <description>La date en test n&apos;est modifiable que par les équipes de Kerensen</description>
        <errorConditionFormula>ISCHANGED( Date_en_test__c ) &amp;&amp;  $User.CompanyName = &quot;K par K&quot;</errorConditionFormula>
        <errorDisplayField>Date_en_test__c</errorDisplayField>
        <errorMessage>La date en test n&apos;est modifiable que par les équipes de Kerensen</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Modification_config</fullName>
        <active>false</active>
        <description>Si le statut est réalisé, il faut indiquer les modifications</description>
        <errorConditionFormula>ISPICKVAL( Statut__c, &quot;Réalisé&quot;) &amp;&amp; ( Pas_de_modification__c = False &amp;&amp; Liste_des_modifications__c = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Pas_de_modification__c</errorDisplayField>
        <errorMessage>Veuillez renseigner l&apos;un des 2 champs &quot;Pas de modification&quot; ou &quot;Liste des modifications&quot;</errorMessage>
    </validationRules>
