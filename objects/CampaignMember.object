    <validationRules>
        <fullName>inx__permit_adding_members</fullName>
        <active>true</active>
        <description>Do not allow adding members to connected campaigns which have &quot;keep in sync&quot; disabled.</description>
        <errorConditionFormula>LEN( TEXT(CreatedDate ))= 0  &amp;&amp;  Campaign.inx__connect_with_Inxmail__c &amp;&amp;  Campaign.inx__keep_in_sync__c  = false</errorConditionFormula>
        <errorMessage>You cannot add members to connected campaigns. Please disconnect the campaign first.</errorMessage>
    </validationRules>
