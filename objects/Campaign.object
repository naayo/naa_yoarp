    </fields>
    <fields>
        <fullName>inx__connection_status__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF( inx__connect_with_Inxmail__c  = false, $Label.inx__Campaign_disconnected_from_Inxmail_Professional, 
IF( inx__connection_status_number__c  = -1,  $Label.inx__Invalid_Listname ,
IF( inx__connection_status_number__c  = -2,  $Label.inx__inx_connection_failed ,
IF( inx__connection_status_number__c  = -3,  $Label.inx__inx_not_active ,
IF( inx__connection_status_number__c  = 1, $Label.inx__Campaign_completely_sync_with_Inxmail_Professional,
IF( inx__connection_status_number__c  = 2, $Label.inx__Campaign_synchronizing_with_Inxmail_Professional,  
IF( inx__connection_status_number__c  = 3   &amp;&amp; inx__connect_with_Inxmail__c  = false, $Label.inx__Campaign_disconnected_from_Inxmail_Professional,  
IF ( NOT ISBLANK( inx__list_id__c ),  $Label.inx__Campaign_connected_with_Inxmail_Professional ,  $Label.inx__Campaign_disconnected_from_Inxmail_Professional ))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Affiche l&apos;état de connexion avec Inxmail Professional</inlineHelpText>
        <label>Connection Status</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
